#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/.pnpm/autoprefixer@9.8.8/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/.pnpm/autoprefixer@9.8.8/node_modules/autoprefixer/bin/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/.pnpm/autoprefixer@9.8.8/node_modules/autoprefixer/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/.pnpm/autoprefixer@9.8.8/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules:/home/nxtwave/reactjs/coding-practices/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/autoprefixer/bin/node_modules:/home/nxtwave/reactjs/coding-practices/welcomeApp/node_modules/autoprefixer/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else 
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
